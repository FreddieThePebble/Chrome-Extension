{
  "manifest_version": 3,
  "name": "YouTube: Rickrolled",
  "description": "Rick-Roll Prank",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "host_permissions": [
    "http://www.youtube.com/*",
    "https://www.youtube.com/*"
  ]
}
